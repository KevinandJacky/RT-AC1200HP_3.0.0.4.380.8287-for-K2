
#
## Ralink RT2880 board
#
platform-$(CONFIG_RALINK_RT2880)+= ralink/
cflags-$(CONFIG_RALINK_RT2880)	+= -I$(srctree)/arch/mips/include/asm/mach-ralink
load-$(CONFIG_RALINK_RT2880)	+= 0xffffffff88000000

